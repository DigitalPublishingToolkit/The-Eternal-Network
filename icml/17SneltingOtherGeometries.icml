              <PathPointType Anchor="212.50000 219.50000" LeftDirection="212.50000 219.50000" RightDirection="212.50000 219.50000" />
              <PathPointType Anchor="212.50000 -219.50000" LeftDirection="212.50000 -219.50000" RightDirection="212.50000 -219.50000" />
            </PathPointArray>
          </GeometryPathType>
        </PathGeometry>
      </Properties>
